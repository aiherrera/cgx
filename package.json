{
  "name": "cgx",
  "version": "0.0.1",
  "description": "Generate all recommended files for the community standards",
  "author": "Jeroen Ouwehand",
  "repository": "https://github.com/jeroenouw/cgx",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "cgx": "bin/cgx"
  },
  "keywords": [
    "cli",
    "cgx",
    "community",
    "typescript",
    "javascript",
    "nodejs"
  ],
  "scripts": {
    "start": "npm run build &&  npm run global",
    "build": "tsc -p .",
    "clean": "rm -rf ./lib",
    "global": "sudo npm i -g && cgx",
    "refresh": "rm -rf ./node_modules ./package-lock.json ./lib && npm install",
    "tscov": "tscov --min-coverage 90",
    "tscov:d": "tscov --details"
  },
  "dependencies": {
    "@types/fs-extra": "^5.0.5",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "commander": "^2.20.0",
    "figlet": "^1.2.1",
    "fs-extra": "^8.0.0",
    "glob": "7.1.3",
    "inquirer": "^6.3.1",
    "inversify": "^5.0.1",
    "minimist": "^1.2.0",
    "ora": "^3.4.0",
    "path": "^0.12.7",
    "prompts": "^2.0.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@liftr/tscov": "^1.4.0",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.0.0",
    "nodemon": "^1.19.0",
    "ts-loader": "^6.0.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
